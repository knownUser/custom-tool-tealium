{
    "id": "audiencestream_extract",
    "title": "AudienceStream Extract",
    "description": "This tool downloads the attributes and audiences from AudienceStream Profile",
    "icon_url": "https://knownuser.github.io/custom-tool-tealium/one-punch-man-saitama.jpg",
    "scripts": {
        "attributes-by-text": {
            "title": "Attributes by Label",
            "input": {
                "attributeLabel": {
                    "type": "text",
                    "placeholder": "Filter Name",
                    "label": "Attribute Label:"
                },
                "auto_inject": true,
                "js": "https://knownuser.github.io/custom-tool-tealium/datalayerDownload/datalayer-download.js",
                "remote_js": true,
                "template": "https://knownuser.github.io/custom-tool-tealium/datalayerDownload/datalayer-download.html",
                "remote_template": true
            },
            "audiences-by-text": {
                "title": "Audiences by Label",
                "input": {
                    "myTitle": {
                        "type": "text",
                        "placeholder": "Filter Name",
                        "label": "Audience Label:"
                    },
                    "auto_inject": true,
                    "js": "https://knownuser.github.io/custom-tool-tealium/datalayerDownload/datalayer-download.js",
                    "remote_js": true,
                    "template": "https://knownuser.github.io/custom-tool-tealium/datalayerDownload/datalayer-download.html",
                    "remote_template": true
                }
            }
        }
    }
}